unit UPiloto;

interface

uses
    UEntidade
  ;

type
  TPiloto = class
  private
    FNome: String;
    FDataNascimento: TDate;

  public
    property NOME: String read FNome write FNome;
    property DATA_NASCIMENTO: TDate read FDataNascimento write FDataNascimento;

  end;

const
  TBL_PILOTO                  = 'PILOTO';
  FLD_PILOTO_NOME             = 'NOME';
  FLD_PILOTO_DATA_NASCIMENTO  = 'DATA_NASCIMENTO';

implementation

end.
